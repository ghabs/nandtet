/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=b, b=sel,out=nana);
    Not(in=sel,out=notsel);
    Nand(a=notsel,b=a,out=nanb);
    Nand(a=nana,b=nanb,out=out);
}